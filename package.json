{
  "name": "sendy-backend",
  "version": "1.0.0",
  "description": "express graphql using aws-lambda with @apollo/server",
  "main": "server.ts",
  "author": "Kamal Aryal",
  "license": "MIT",
  "scripts": {
    "func": "serverless deploy function --function graphql",
    "deploy": "serverless deploy",
    "deploy:develop": "sls deploy --verbose --config serverless-development.yml",
    "deploy:staging": "sls deploy --verbose --config serverless-staging.yml",
    "remove": "serverless remove",
    "offline": "serverless offline",
    "build": "tsc",
    "start": "node dist/index.js",
    "build:watch": "tsc --watch",
    "start:watch": "nodemon dist/index.js",
    "start:dev": "nodemon --exec ts-node --type-check src/index.ts",
    "clean": "rm -rf ./dist",
    "fix": "yarn fix:eslint && yarn fix:prettier",
    "fix:eslint": "eslint --fix \"**/*.*\"",
    "fix:prettier": "prettier --write \"**/*.*\"",
    "lint": "yarn lint:eslint && yarn lint:prettier",
    "lint:eslint": "eslint \"**/*.*\"",
    "lint:prettier": "prettier --check \"**/*.*\"",
    "sequelize:core:migrate": "sequelize --options-path ./.sequelize-core db:migrate",
    "sequelize:core:migrate:undo": "sequelize --options-path ./.sequelize-core db:migrate:undo",
    "sequelize:core:migrate:undo:all": "sequelize --options-path ./.sequelize-core db:migrate:undo:all",
    "sequelize:core:seed:all": "sequelize --options-path ./.sequelize-core db:seed:all",
    "sequelize:tenant:migrate": "sequelize --options-path ./.sequelize-tenant db:migrate",
    "sequelize:tenant:migrate:undo": "sequelize --options-path ./.sequelize-tenant db:migrate:undo",
    "sequelize:tenant:migrate:undo:all": "sequelize --options-path ./.sequelize-tenant db:migrate:undo:all",
    "sequelize:tenant:seed:all": "sequelize --options-path ./.sequelize-tenant db:seed:all",
    "sequelize:core:migrate:staging": "sequelize --options-path ./.sequelize-core db:migrate --env staging",
    "sequelize:core:migrate:undo:staging": "sequelize --options-path ./.sequelize-core db:migrate:undo --env staging",
    "sequelize:core:migrate:undo:all:staging": "sequelize --options-path ./.sequelize-core db:migrate:undo:all --env staging",
    "sequelize:core:seed:all:staging": "sequelize --options-path ./.sequelize-core db:seed:all --env staging",
    "sequelize:tenant:migrate:staging": "sequelize --options-path ./.sequelize-tenant db:migrate --env staging",
    "sequelize:tenant:migrate:undo:staging": "sequelize --options-path ./.sequelize-tenant db:migrate:undo --env staging",
    "sequelize:tenant:migrate:undo:all:staging": "sequelize --options-path ./.sequelize-tenant db:migrate:undo:all --env staging",
    "sequelize:tenant:seed:all:staging": "sequelize --options-path ./.sequelize-tenant db:seed:all --env staging",
    "start-core": "serverless offline start --config serverless-core.yml",
    "start-tenant": "serverless offline start --config serverless-tenant.yml",
    "migrate-up-core": "serverless migrations up --config serverless-core.yml",
    "migrate-up-tenant": "serverless migrations up --config serverless-tenant.yml",
    "migrate-down-core": "serverless migrations down --config serverless-core.yml",
    "migrate-down-tenant": "serverless migrations down --config serverless-tenant.yml",
    "migrate-reset-core": "serverless migrations reset --config serverless-core.yml",
    "migrate-reset-tenant": "serverless migrations reset --config serverless-tenant.yml",
    "migrate-list-core": "serverless migrations list --config serverless-core.yml",
    "migrate-list-tenant": "serverless migrations list --config serverless-tenant.yml",
    "seed-core": "sequelize db:seed:all --config src/config/database.js --seeders-path src/core/seeders",
    "undo-seed-core": "sequelize db:seed:undo:all --config src/config/database.js --seeders-path src/core/seeders",
    "seed-tenant": "sequelize db:seed:all --config src/config/tenantDatabase.js --seeders-path src/tenant/seeders",
    "undo-seed-tenant": "sequelize db:seed:undo:all --config src/config/tenantDatabase.js --seeders-path src/tenant/seeders"
  },
  "dependencies": {
    "@apollo/server": "^4.3.2",
    "@apollo/subgraph": "^2.4.1",
    "@as-integrations/aws-lambda": "^2.0.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.258.0",
    "@aws-sdk/client-s3": "^3.353.0",
    "@aws-sdk/client-ses": "^3.276.0",
    "@aws-sdk/client-sqs": "^3.496.0",
    "@jorgeferrero/stream-to-buffer": "^2.0.6",
    "@vendia/serverless-express": "^4.10.4",
    "amazon-cognito-identity-js": "^6.1.2",
    "aws-jwt-verify": "^3.3.0",
    "aws-sdk": "^2.1442.0",
    "aws-serverless-express": "^3.4.0",
    "axios": "^1.3.3",
    "cls-hooked": "^4.2.2",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "graphql": "^16.6.0",
    "graphql-redis-subscriptions": "^2.6.0",
    "graphql-tag": "^2.12.6",
    "graphql-upload": "^12.0.0",
    "graphql-ws": "^5.14.0",
    "husky": "^8.0.3",
    "joi": "^17.7.0",
    "ksuid": "^3.0.0",
    "middy": "^0.36.0",
    "moment-timezone": "^0.5.43",
    "node-cron": "^3.0.2",
    "node-schedule": "^2.1.1",
    "nodemailer": "^6.9.7",
    "pg": "^8.8.0",
    "qrcode": "^1.5.3",
    "redis": "^4.6.5",
    "sequelize": "^6.28.0",
    "sequelize-cli": "^6.5.2",
    "serverless-sequelize-migrations": "^1.1.2",
    "slug": "^8.2.2",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/register": "^7.18.9",
    "@types/aws-lambda": "^8.10.130",
    "@types/cls-hooked": "^4.3.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/graphql-upload": "^8.0.12",
    "@types/node": "^18.11.18",
    "@types/node-cron": "^3.0.8",
    "@types/node-schedule": "^2.1.0",
    "@types/nodemailer": "^6.4.14",
    "@types/qrcode": "^1.5.5",
    "@types/sequelize": "^4.28.14",
    "@types/slug": "^5.0.3",
    "@types/ws": "^8.5.5",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "eslint": "^8.34.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.4",
    "rimraf": "^4.1.2",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-offline": "^12.0.4",
    "serverless-openapi-joi": "^0.2.7",
    "serverless-plugin-typescript": "^2.1.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "repository": "git@github.com:Varosa-Technology/sendy-backend.git",
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  }
}